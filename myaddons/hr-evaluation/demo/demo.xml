			<field name="level_id" ref="p1" />
			<field name="post_id" ref="post_dev" />
			<field name="description">达到本岗位知识技能的基本要求，能够在他人指导下、按照标准流程和方法完成较明确的专业性任务。
			</field>
		</record>

		<record id="post_dev_p2" model="hr.postlevel">
