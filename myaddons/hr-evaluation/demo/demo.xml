			<field name="level_id" ref="p6" />
			<field name="post_id" ref="post_dev" />
			<field name="description">为本岗位所在领域的专家，能负责解决影响公司关键业务的难题，为公司做出持续重要贡献。
			</field>
		</record>

		<record id="post_dev_p7" model="hr.postlevel">
